{
  "name": "fatimas-kitchen-enterprise-v2",
  "version": "2.0.26",
  "private": true,
  "author": "Fatima's Kitchen Global",
  "description": "A High-Performance, SEO-Optimized Luxury Food Portal with Real-time Inventory and Multi-branch Support.",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-router-dom": "^6.22.0",
    
    "/* -- UI & LUXURY DESIGN -- */": "v1",
    "framer-motion": "^11.0.8",
    "styled-components": "^6.1.8",
    "lucide-react": "^0.344.0",
    "react-icons": "^5.0.1",
    "swiper": "^11.0.7",
    "canvas-confetti": "^1.9.2",
    "react-reveal": "^1.2.2",
    
    "/* -- STATE & DATA MANAGEMENT -- */": "v1",
    "@reduxjs/toolkit": "^2.2.1",
    "react-redux": "^9.1.0",
    "axios": "^1.6.7",
    "react-query": "^3.39.3",
    "lodash": "^4.17.21",
    
    "/* -- UTILITIES & ANIMATION -- */": "v1",
    "clsx": "^2.1.0",
    "tailwind-merge": "^2.2.1",
    "react-scroll": "^1.9.0",
    "react-fast-marquee": "^1.6.4",
    
    "/* -- FORMS & VALIDATION -- */": "v1",
    "formik": "^2.4.5",
    "yup": "^1.4.0",
    "react-toastify": "^10.0.4",
    "sweetalert2": "^11.10.5",
    
    "/* -- PERFORMANCE & ANALYTICS -- */": "v1",
    "web-vitals": "^3.5.2",
    "react-intersection-observer": "^9.8.1",
    "nprogress": "^0.2.0"
  },
  "scripts": {
    "dev": "react-scripts start",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "format": "prettier --write \"src/**/*.{js,jsx,css,md}\"",
    "clean": "rm -rf build node_modules",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "production": "npm run build && serve -s build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "error",
      "react/prop-types": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^6.1.1",
    "prettier": "^3.2.5",
    "eslint-plugin-react": "^7.33.2",
    "source-map-explorer": "^2.5.3",
    "serve": "^14.2.1"
  },
  "keywords": [
    "premium-food-app",
    "react-luxury-ui",
    "gold-theme-ecommerce",
    "fatimas-kitchen-tech-stack",
    "high-conversion-landing-page"
  ],
  "homepage": "https://fatimaskitchen.com"
}
